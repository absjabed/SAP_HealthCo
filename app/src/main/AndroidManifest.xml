<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.absjbd.sap_healthco">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon">
        <activity android:name=".Activity.MainActivity" />
        <activity android:name=".Activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Activity.HealthActivity" />
        <activity android:name=".Activity.HospitalActivity" />
        <activity android:name=".Activity.DoctorActivity" />
        <activity android:name=".Activity.InfoActivity" />
        <activity android:name=".Activity.DiseaseActivity" />
        <activity android:name=".Activity.ChildDiseaseActivity" />
    </application>

</manifest>